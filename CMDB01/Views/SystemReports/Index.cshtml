@model IEnumerable<CMDB01.Models.SystemReport>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ReportName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ReportPath)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ReportCategory)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DS1Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DS2Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DS3Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DS4Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DS5Name)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.ReportName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ReportPath)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ReportCategory)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DS1Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DS2Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DS3Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DS4Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DS5Name)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
